{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jquery": true,
    "mocha": true
  },
  "plugins": [
    "mocha",
    "jquery",
    "dollar-sign",
    "jquery-selectors"
  ],
  "globals": {
    "ekino": true
  },
  "extends": "airbnb",
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-unused-vars": 1,
    "curly": 2,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "eqeqeq": 2,
    "new-cap": 2,
    "camelcase": 2,
    "quotes": [2, "single"],
    "no-trailing-spaces": 2,
    "dollar-sign/dollar-sign": [2, "ignoreProperties"],
    "jquery/no-ajax": 2,
    "jquery/no-attr": 0,
    "jquery/no-bind": 2,
    "jquery/no-class": 0,
    "jquery/no-clone": 2,
    "jquery/no-css": 2,
    "jquery/no-deferred": 2,
    "jquery/no-delegate": 2,
    "jquery/no-find": 0,
    "jquery/no-global-eval": 2,
    "jquery/no-html": 0,
    "jquery/no-in-array": 2,
    "jquery/no-load": 2,
    "jquery/no-map": 2,
    "jquery/no-merge": 2,
    "jquery/no-param": 2,
    "jquery/no-parent": 0,
    "jquery/no-parents": 2,
    "jquery/no-parse-html": 2,
    "jquery/no-proxy": 2,
    "jquery/no-ready": 2,
    "jquery/no-serialize": 2,
    "jquery/no-size": 2,
    "jquery/no-submit": 2,
    "jquery/no-text": 0,
    "jquery/no-toggle": 2,
    "jquery/no-trigger": 2,
    "jquery/no-trim": 2,
    "jquery/no-when": 2,
    "jquery/no-wrap": 2
  }
}
